{
  "name": "Busimark",
  "manifest_version": 2,
  "version": "0.1",

  "permissions": ["bookmarks", "storage", "tabs"],

  "browser_action": {
    "default_popup": "main.html",
    "default_title": "Busimark",
    "default_icon": "beasts-32.png"
  },

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      }
    }
  }
}
